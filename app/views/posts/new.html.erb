<%= form_with model: @post do |f| %>
    By: <%= f.collection_select :user_id, @users, :id, :name %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :content %>
    <%= f.text_area :content %>
    <%= f.label :image %>
    <%= f.text_field :image %>
    <%= f.submit %>
<% end %>