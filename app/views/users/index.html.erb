<% @users.each do |user| %>
<div class="row">
    <div class="col-sm-3">
        <div class="well">
           <p><%= user.name %></p>
           <img src="bird.jpg" class="img-circle" height="55" width="55" alt="Avatar">
        </div>
    </div>
    <div class="col-sm-9">
          <div class="well">
            <p><%= user.bio %></p>
          </div>
    </div>
</div>
<% end %>